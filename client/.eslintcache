Context);\n\n  useEffect(() => {\n    fetch('/myposts', {\n      headers: {\n        Authorization: 'Bearer ' + localStorage.getItem('jwt'),\n      },\n    })\n      .then(res => res.json())\n      .then(result => {\n        console.log(result);\n        setMyPics(result.myposts);\n      });\n  }, []);\n\n  return (\n    <React.Fragment>\n      <Nav />\n      <ProfileWrapper>\n        <ProfileHeader>\n          <ProfilePicture\n            src=\"https://images.unsplash.com/photo-1578173257188-2c095b0aef8b?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=800&q=80\"\n            alt=\"profile-pic\"\n          />\n          <ProfileInfo>\n            <h2 style={{ fontSize: '35px', fontWeight: '100' }}>ocean_fuaga</h2>\n            <ProfileStats>\n              <div style={{ marginRight: '1rem' }}>\n                <h3 style={{ fontWeight: '400' }}>\n                  <span style={{ fontWeight: '600' }}>10</span> posts\n                </h3>\n              </div>\n              <div style={{ marginRight: '1rem' }}>\n                <h3 style={{ fontWeight: '400' }}>\n                  <span style={{ fontWeight: '600' }}>10</span> followers\n                </h3>\n              </div>\n              <div style={{ marginRight: '1rem' }}>\n                <h3 style={{ fontWeight: '400' }}>\n                  <span style={{ fontWeight: '600' }}>10</span> following\n                </h3>\n              </div>\n            </ProfileStats>\n          </ProfileInfo>\n        </ProfileHeader>\n        <Gallery>\n          {mypics.map(post => (\n            <Image image={post.photo} key={post.title}></Image>\n          ))}\n        </Gallery>\n      </ProfileWrapper>\n    </React.Fragment>\n  );\n};\n\nexport default Profile;\n","/Users/ocean/repos/cookbooks/client/src/components/screens/Home.js",[],"/Users/ocean/repos/cookbooks/client/src/components/screens/SignIn.js",["65"],"import React, { useState, useContext } from 'react';\nimport { 